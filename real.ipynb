import turtle

turtle.width(5)
turtle.forward(1)
turtle.color("red")
turtle.circle(30)
turtle.penup()
turtle.forward(60)
turtle.pendown()
turtle.circle(30)
turtle.penup()
turtle.forward(60)
turtle.pendown()
turtle.circle(30)
turtle.color("blue")
turtle.left(-110)
turtle.forward(200)
turtle.right(145)
turtle.forward(200)
turtle.penup()
turtle.right(10)
turtle.forward(60)
turtle.pendown()
turtle.color("green")
turtle.circle(-70,190)

turtle.mainloop()
